    "version": "0.2.0",
    "configurations": [
        {
            "name": "backend1 code executor",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/backend/code_executor",
            "module": "uvicorn",
            "args": [
                "app:app",
                "--reload",
                "--host", "0.0.0.0",
                "--port", "8000",

            ],
            "jinja": true
        },

        {
            "name": "backend neuralabs",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/neuralabs-backend",
            "program": "${workspaceFolder}/neuralabs-backend/run.py",
            "console": "integratedTerminal",
            "jinja": true
        },


        {
            "type": "msedge",
            "request": "launch",
            "name": "Edge - Launch 3000",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}"
        },
 
        {
            "name": "move_contract_frontend_sid",
            "type": "msedge",
            "request": "attach",
            "port": 9223,
            "webRoot": "/home/sid/projects/06_02 NeuraLabs/neuralabs-sui/mov_contract/frontend_interaction", 
        }

    ]
}